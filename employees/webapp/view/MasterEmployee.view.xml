<mvc:View
    controllerName="logaligroup.employees.controller.MasterEmployee"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:layout="sap.ui.layout">

                <Page id="page" title="{i18n>title}">
                    <content>
                     <HBox id="HBOX1" wrap="Wrap">
                        <Label id="labelEmployee" labelFor="inputEmployee" text="{i18n>labelEmployee}" class="sapUiSmallMargin" /> 
                        <Input id="inputEmployee" value="{jsonCountries>/EmployeeId}" maxLength="6" width="200px" class="sapUiSmallMarginEnd" />
                        <Label id="labelCountry" labelFor="slCountry" text="{i18n>labelCountry}" class="sapUiSmallMargin" visible="true"/> 
                        <Select id="slCountry" selectedKey="{jsonCountries>/CountryKey}" forceSelection="false" items="{jsonCountries>/ListCountry}" visible="true">
                            <core:Item id="_IDGenItem1" key="{jsonCountries>Key}"  text="{jsonCountries>Text}"/>
                        </Select>
                        <Button id="btnFilter" text="{i18n>btnFilter}"  icon="sap-icon://search" press="onFilter" class="sapUiSmallMargin"/>
                        <Button id="btnClearFilter" text="{i18n>btnClearFilter}"  icon="sap-icon://clear-filter" press="onClearFilter" class="sapUiSmallMargin"/>
                     </HBox>

                      <core:Fragment fragmentName="logaligroup.employees.fragment.TableEmployee" type="XML"/>

                    </content>
                </Page>


</mvc:View>
